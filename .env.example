# Core app
APP_PORT=9187
ENV=dev
UVICORN_PROXY_HEADERS=1
FORWARDED_ALLOW_IPS=*
APP_ROOT_PATH=
DETERMINISTIC=0

# Security (generate: openssl rand -hex 32)
JWT_SECRET=change_me_to_strong_random_secret
JWT_ALG=HS256
ACCESS_TOKEN_TTL_MIN=30

# Database / Cache / Queue
POSTGRES_DSN=postgresql+asyncpg://app:app@postgres:5432/app
REDIS_URL=redis://redis:6379/0
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672//

# File storage
FILE_STORAGE=LOCAL
FILE_STORAGE_BASE=/app/storage
REPORT_MAX_SIZE_MB=15

# Celery
CELERY_WORKER_QUEUES=recommendations,recommendations_retry
CELERY_LOG_LEVEL=DEBUG
CELERY_TASK_ALWAYS_EAGER=0
CELERY_EAGER_PROPAGATES_EXCEPTIONS=0

# Testing
ALLOW_EXTERNAL_NETWORK=1
DETERMINISTIC_SEED=42
FROZEN_TIME=

# CORS
CORS_ALLOW_ALL=false
ALLOWED_ORIGINS=

# Logging
LOG_LEVEL=DEBUG
LOG_MASK_SECRETS=1

# VPN (supports: wireguard, awg, openvpn, hysteria2)
VPN_ENABLED=0
VPN_TYPE=hysteria2
# WireGuard/AWG settings
WG_CONFIG_PATH=
WG_INTERFACE=awg0
# OpenVPN settings (legacy)
OPENVPN_CONFIG_PATH=
OPENVPN_INTERFACE=tun0
# Hysteria2 settings (recommended)
HYSTERIA2_URI=hysteria2://SenpaiCat2407@vpn.vibe-labs.ru:8443/?sni=vpn.vibe-labs.ru
HYSTERIA2_SOCKS5_PORT=1080
HYSTERIA2_HTTP_PORT=8080
HYSTERIA2_CONFIG_PATH=/tmp/hysteria2.yaml
# Routing settings (for WireGuard/AWG/OpenVPN, not used with Hysteria2)
VPN_ROUTE_MODE=domains
VPN_ROUTE_DOMAINS=generativelanguage.googleapis.com
VPN_BYPASS_CIDRS=172.16.0.0/12,10.0.0.0/8,192.168.0.0/16

# Gemini / AI
GEMINI_API_KEYS=
GEMINI_MODEL_TEXT=gemini-2.5-flash
GEMINI_MODEL_VISION=gemini-2.5-flash
GEMINI_QPS_PER_KEY=0.5
GEMINI_BURST_MULTIPLIER=2.0
GEMINI_TIMEOUT_S=30
GEMINI_STRATEGY=ROUND_ROBIN
AI_RECOMMENDATIONS_ENABLED=1
AI_VISION_FALLBACK_ENABLED=1
